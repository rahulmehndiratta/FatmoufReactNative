// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true,                                 /* Skip type checking all .d.ts files. */
  
 "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@redux/*": [
        "./redux/*"
      ],
      "@screens/*": [
        "./screens/"
      ],
      "@screenName": [
        "./navigators/registerScreens"
      ],
      "@api": [
        "./api/"
      ],
      "@Utils": [
        "./utils/Utils.tsx"
      ],
      "@images": [
        "./assets/images/"
      ],
      "@Navigator": [
        "./navigators/Navigator"
      ],
      "@MyAsyncStorage": [
        "./storage/MyAsyncStorage"
      ],
      "@keyName": [
        "./storage/keyName"
      ],
      "@styles": [
        "./styles/styles"
      ],
      "@strings": [
        "./styles/strings"
      ],
      "@ApiClient": [
        "./api/ApiClient"
      ]
    }
}
}
